#include <stdio.h>

int main () {
    printf("%c %c %c %c\n", 0x6, 0x5, 0xB, 0xE);
    printf("%c %c %c %c\n", 0x1, 0x16, 0x1A, 0xF);
    printf("Piege = %c\n",0x5);
    printf("Oiseau = %c\n",0xE);
    printf("Snoopy = %c\n",0xB);
    printf("Bloc fixe = %c\n",0xF);
    printf("Bloc poussable = %c\n",0x1A);
    printf("Bloc cassable = %c\n",0x6);
    return 0;
}
